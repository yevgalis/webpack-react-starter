{
  "name": "react-starter-kit",
  "version": "3.1.7",
  "description": "Initial webpack setup for React projects",
  "author": "Evgeniy Alishevets",
  "repository": "https://github.com/yevgalis/webpack-react-starter",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react",
    "typescript",
    "babel",
    "eslint",
    "stylelint"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "eslint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "stylelint": "stylelint \"src/{**/*,*}.{css,scss}\"",
    "stylelint-fix": "stylelint \"src/{**/*,*}.{css,scss}\" --fix --formatter=verbose"
  },
  "dependencies": {
    "@babel/runtime": "7.20.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "babel-loader": "9.1.2",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "cssnano": "5.1.14",
    "eslint": "8.34.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "3.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "2.7.2",
    "postcss": "8.4.21",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "15.1.0",
    "postcss-loader": "7.0.2",
    "postcss-preset-env": "7.8.3",
    "postcss-safe-parser": "6.0.0",
    "robotstxt-webpack-plugin": "8.0.0",
    "sass": "1.58.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-webpack-plugin": "3.3.0",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.2",
    "typescript": "4.8.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "npm run eslint-fix"
    ],
    "*.+(css|scss)": [
      "npm run stylelint-fix"
    ]
  },
  "engines": {
    "node": "14"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
