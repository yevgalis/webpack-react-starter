{
  "name": "webpack-react-starter",
  "version": "2.0.0",
  "description": "Initial webpack setup for React projects",
  "author": "Evgeniy Alishevets",
  "repository": "https://github.com/yevgalis/webpack-react-starter",
  "homepage": "https://github.com/yevgalis/webpack-react-starter",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react",
    "babel",
    "eslint",
    "stylelint",
    "postcss"
  ],
  "scripts": {
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix --formatter=verbose",
    "eslint": "eslint --ext .jsx --ext .js src/",
    "eslint:fix": "eslint --fix --ext .jsx --ext .js src/",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack"
  },
  "dependencies": {
    "@babel/runtime": "7.17.2",
    "dotenv": "14.2.0",
    "dotenv-webpack": "6.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-logical-assignment-operators": "7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@svgr/webpack": "5.5.0",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "6.4.0",
    "cross-env": "7.0.3",
    "css-loader": "5.2.7",
    "cssnano": "5.0.17",
    "eslint": "8.9.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.6.2",
    "postcss": "8.4.6",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "14.0.2",
    "postcss-loader": "4.2.0",
    "postcss-preset-env": "7.4.1",
    "postcss-safe-parser": "6.0.0",
    "sass": "1.49.8",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "stylelint": "14.5.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-scss": "4.1.0",
    "svg-url-loader": "7.1.1",
    "terser-webpack-plugin": "4.2.3",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "npm run eslint:fix"
    ],
    "*.+(css|scss)": [
      "npm run stylelint:fix"
    ]
  },
  "engines": {
    "node": "12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
}
