{
  "name": "react-starter-kit",
  "version": "4.0.0",
  "description": "Initial webpack setup for React projects",
  "author": "Evgeniy Alishevets",
  "repository": "https://github.com/yevgalis/webpack-react-starter",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "eslint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "stylelint": "stylelint \"src/{**/*,*}.css\"",
    "stylelint-fix": "stylelint \"src/{**/*,*}.css\" --fix --formatter=verbose",
    "prettier": "prettier . --check",
    "prettier-fix": "prettier . --write",
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "prepare": "husky"
  },
  "dependencies": {
    "@babel/runtime": "7.26.0",
    "buffer": "6.0.3",
    "normalize.css": "8.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.27.0"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@types/node": "22.10.7",
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "8.20.0",
    "babel-loader": "9.2.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "cross-env": "7.0.3",
    "css-loader": "6.11.0",
    "cssnano": "7.0.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-webpack-plugin": "4.2.0",
    "html-webpack-plugin": "5.6.3",
    "husky": "9.1.7",
    "lint-staged": "15.4.1",
    "mini-css-extract-plugin": "2.9.2",
    "postcss": "8.5.1",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-import": "16.1.0",
    "postcss-loader": "8.1.1",
    "postcss-preset-env": "9.5.14",
    "postcss-safe-parser": "7.0.1",
    "prettier": "3.4.2",
    "robotstxt-webpack-plugin": "8.0.0",
    "style-loader": "4.0.0",
    "stylelint": "16.12.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-webpack-plugin": "5.0.1",
    "svg-url-loader": "8.0.0",
    "terser-webpack-plugin": "5.3.11",
    "ts-loader": "9.5.2",
    "typescript": "5.7.3",
    "webpack": "5.97.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.0"
  },
  "engines": {
    "node": "18"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ]
}
