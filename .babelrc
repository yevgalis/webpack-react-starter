{
  "presets": [
    ["@babel/preset-react", {
      "runtime": "automatic"
    }],
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver", {
        "root": ["./src"]
      }
    ]
  ]
}
